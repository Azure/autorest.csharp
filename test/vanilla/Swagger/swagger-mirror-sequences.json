{
    "swagger": "2.0",
    "info":
    {
        "version": "1.0.0",
        "title": "Sequence Request Response Test",
        "description": "A sample API that uses a petstore as an example to demonstrate features in the swagger-2.0 specification",
        "termsOfService": "n/a",
        "contact":
        {
            "name": "n/a",
            "email": "foo@example.com",
            "url": "n/a"
        }
        ,
        "license":
        {
            "name": "MIT",
            "url": "http://github.com/gruntjs/grunt/blob/master/LICENSE-MIT"
        }
    }
    ,
    "host": "petstore.swagger.wordnik.com",
    "basePath": "/api",
    "schemes": [
    "http"
    ],
    "consumes": [
    "application/json"
    ],
    "produces": [
    "application/json"
    ],
    "paths":
    {
        "/pets":
        {
            "post":
            {
                "description": "Creates a new pet in the store.  Duplicates are allowed",
                "operationId": "addPet",
                "produces": [
                "application/json"
                ],
                "parameters": [
                {
                    "name": "pets",
                    "in": "body",
                    "description": "Pets to add to the store",
                    "required": true,
                    "schema":
                    {
                        "type": "array",
                        "items":
                        {
                            "$ref": "#/definitions/pet"
                        }
                    }
                }
                ],
                "responses":
                {
                    "200":
                    {
                        "description": "pet response",
                        "schema":
                        {
                            "type": "array",
                            "items":
                            {
                                "$ref": "#/definitions/pet"
                            }
                        }
                    }
                    ,
                    "default":
                    {
                        "description": "unexpected error",
                        "schema":
                        {
                            "$ref": "#/definitions/errorModel"
                        }
                    }
                }
            }
        }
        ,
        "/primitives":
        {
            "post":
            {
                "description": "Adds new pet stylesin the store.  Duplicates are allowed",
                "operationId": "addPetStyles",
                "produces": [
                "application/json"
                ],
                "parameters": [
                {
                    "name": "petStyle",
                    "in": "body",
                    "description": "Pet style to add to the store",
                    "required": true,
                    "schema":
                    {
                        "type": "array",
                        "items":
                        {
                            "description": "The individual pet styles",
                            "type": "integer"
                        }
                    }
                }
                ],
                "responses":
                {
                    "200":
                    {
                        "description": "pet styles response",
                        "schema":
                        {
                            "type": "array",
                            "items":
                            {
                                "description": "The individual pet styles",
                                "type": "integer"
                            }
                        }
                    }
                    ,
                    "default":
                    {
                        "description": "unexpected error",
                        "schema":
                        {
                            "type": "array",
                            "items":
                            {
                                "$ref": "#/definitions/errorModel"
                            }
                        }
                    }
                }
            }
            ,
            "put":
            {
                "description": "Updates new pet stylesin the store.  Duplicates are allowed",
                "operationId": "updatePetStyles",
                "produces": [
                "application/json"
                ],
                "parameters": [
                {
                    "name": "petStyle",
                    "in": "body",
                    "description": "Pet style to add to the store",
                    "required": true,
                    "schema":
                    {
                        "type": "array",
                        "items":
                        {
                            "description": "The individual pet styles",
                            "type": "integer"
                        }
                    }
                }
                ],
                "responses":
                {
                    "200":
                    {
                        "description": "pet styles response",
                        "schema":
                        {
                            "type": "array",
                            "items":
                            {
                                "description": "The individual pet styles",
                                "type": "integer"
                            }
                        }
                    }
                    ,
                    "default":
                    {
                        "description": "unexpected error",
                        "schema":
                        {
                            "type": "array",
                            "items":
                            {
                                "$ref": "#/definitions/errorModel"
                            }
                        }
                    }
                }
            }

        }
    }
    ,
    "definitions":
    {
        "pet-style":
        {
            "required": [
            "name"
            ],
            "properties":
            {
                "name":
                {
                    "type": "string"
                }
            }
        }
        ,
        "pet":
        {
            "required": [
            "id",
            "name"
            ],
            "properties":
            {
                "id":
                {
                    "type": "integer",
                    "format": "int64"
                }
                ,
                "name":
                {
                    "type": "string"
                }
                ,
                "styles":
                {
                    "type": "array",
                    "items":
                    {
                        "$ref": "#/definitions/pet-style"
                    }
                }
                ,
                "tag":
                {
                    "type": "string"
                }
            }
        }
        ,
        "errorModel":
        {
            "required": [
            "code",
            "message"
            ],
            "properties":
            {
                "code":
                {
                    "type": "integer",
                    "format": "int32"
                }
                ,
                "message":
                {
                    "type": "string"
                }
            }
        }
    }
}