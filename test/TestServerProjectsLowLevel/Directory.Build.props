<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!--We don't care about XML doc comments on test types and members. We also don't care about System.ClientModel downgrade in the test project.-->
    <NoWarn>$(NoWarn);CS1591;Nu1605</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <IsTestGenerationSrcProject Condition="!$(MSBuildProjectName.EndsWith('.Tests'))">true</IsTestGenerationSrcProject>
    <IsTestGenerationTestProject Condition="$(MSBuildProjectName.EndsWith('.Tests'))">true</IsTestGenerationTestProject>
  </PropertyGroup>
  <Import Project="..\..\Directory.Build.props" />
</Project>
