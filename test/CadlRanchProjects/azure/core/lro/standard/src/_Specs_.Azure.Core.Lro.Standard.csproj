    <AssemblyTitle>Azure SDK Code Generation _Specs_.Azure.Core.Lro.Standard for Azure Data Plane</AssemblyTitle>
    <Version>1.0.0-beta.1</Version>
    <PackageTags>_Specs_.Azure.Core.Lro.Standard</PackageTags>
    <TargetFrameworks>$(RequiredTargetFrameworks)</TargetFrameworks>
    <IncludeOperationsSharedSource>true</IncludeOperationsSharedSource>
  </PropertyGroup>

  <ItemGroup>
