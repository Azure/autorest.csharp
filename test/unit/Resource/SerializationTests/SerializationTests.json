{
	"swagger": "2.0",
	"info": {
		"version": "1.0.9-abcd",
		"title": "Swagger Sample API"
	},
	"paths": {
		"/pets/": {
			"get": {
				"responses": {
					"200": {
						"description": "pet response"
					}
				}
			}
		}
	},
	"securityDefinitions": {
		"petstore_auth": {
			"type": "oauth2",
			"authorizationUrl": "http://petstore.swagger.io/api/oauth/dialog",
			"flow": "implicit",
			"scopes": {
				"write:pets": "modify pets in your account",
				"read:pets": "read your pets"
			}
		},
		"api_key": {
			"type": "apiKey",
			"name": "api_key",
			"in": "header"
		}
	}
}