{
    "name": "@azure-tools/cadl-csharp",
    "version": "0.1.8",
    "author": "Microsoft Corporation",
    "description": "Cadl library for emitting csharp model from the Cadl REST protocol binding",
    "homepage": "https://github.com/Microsoft/cadl",
    "readme": "https://github.com/Microsoft/cadl/blob/master/README.md",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Azure/autorest.csharp.git"
    },
    "bugs": {
        "url": "https://github.com/Azure/autorest.csharp/issues"
    },
    "keywords": [
        "cadl"
    ],
    "type": "module",
    "main": "dist/src/index.js",
    "scripts": {
        "clean": "rimraf ./dist ./temp",
        "build": "tsc -p .",
        "watch": "tsc -p . --watch",
        "pack": "npx webpack",
        "lint-cadl-library": "cadl compile . --warn-as-error --import @cadl-lang/library-linter --no-emit",
        "test": "mocha",
        "test-official": "c8 mocha --forbid-only",
        "lint": "eslint . --ext .ts --max-warnings=0",
        "lint:fix": "eslint . --fix --ext .ts",
        "prettier:fix": "prettier --write ./src/**/*.ts",
        "prettier": "prettier --check ./src/**/*.ts"
    },
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/js-yaml": "^4.0.5",
        "@types/mocha": "~9.1.0",
        "@types/node": "~16.0.3",
        "@types/prettier": "^2.6.3",
        "c8": "~7.11.0",
        "eslint": "^8.12.0",
        "mocha": "~9.2.0",
        "rimraf": "~3.0.2",
        "typescript": "~4.7.2"
    },
    "dependencies": {
        "@autorest/csharp": "3.0.0-beta.20221124.1",
        "@azure-tools/cadl-autorest": "0.22.0",
        "@azure-tools/cadl-azure-core": "0.9.0",
        "@azure-tools/cadl-dpg": "0.3.0",
        "@cadl-lang/compiler": "0.37.0",
        "@cadl-lang/eslint-config-cadl": "0.4.1",
        "@cadl-lang/eslint-plugin": "0.1.1",
        "@cadl-lang/library-linter": "0.2.2",
        "@cadl-lang/openapi": "0.14.0",
        "@cadl-lang/rest": "0.19.0",
        "@cadl-lang/versioning": "0.10.0",
        "fs-extra": "^10.1.0",
        "js-yaml": "^4.1.0",
        "json-serialize-refs": "^0.0.6-0"
    }
}
